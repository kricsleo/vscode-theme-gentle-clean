	"tokenColors": [
		{
      // key
			"scope": [
				"entity.name.label",
				"entity.other.attribute-name",
				"meta.directive",
				"meta.tag.block.any",
        "meta.object-literal.key",
				"support.type.property-name",
				"variable.object.property",
        "variable.other.object.property",
        "variable.other.property",
        "variable.other.enummember",
				"punctuation.attribute-shorthand"
			],
			"settings": {
				"foreground": "#A5C0AA"
			}
		},
		{
      // string
			"scope": [
				"string",
				"source",
				"text.html",
				"text.html.derivative",
				"meta.property-value",
				"keyword.other.unit",
				"support.type",
				"support.constant.property-value",
				"constant.numeric.css",
				"constant.other.color.rgb-value.hex.css"
			],
			"settings": {
				"foreground": "#5DA994"
			}
		},
		{
      // keyword
			"scope": [
        "punctuation.definition.entity.css",
				"entity.name.tag",
				"entity.other.attribute-name.class",
        "entity.other.attribute-name.pseudo-class.css",
				"support.class.component",
				"storage",
				"keyword",
				"keyword.control",
				"keyword.operator",
				"keyword.operator.word",
				"keyword.operator.new.js",
				"variable.language",
        "variable.scss"
			],
			"settings": {
				"foreground": "#eaac8b"
			}
		},
		{
      // variable
			"scope": [
				"constant",
				"variable.member",
				"variable.parameter",
				"variable.other.constant.object",
				"variable.other.constant.property",
				"variable.other.constant",
				"variable.other.object",
				"variable.other.readwrite",
				"meta.definition.variable",
				"meta.type.declaration.ts",
				"support.constant",
				"support.variable.property",
				"meta.diff",
				"meta.diff.header",
        "string.regexp"
			],
			"settings": {
				"foreground": "#db889a"
			}
		},
		{
      // funciton
			"scope": [
				"variable.function",
				"variable.annotation",
				"support.function",
				"support.macro",
				"support.class",
				"entity.name.function",
				"string.other.link",
				"markup.underline.link",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "meta.command",
        "string.unquoted.argument"
			],
			"settings": {
				"foreground": "#5fa8d3"
			}
		},
    {
      // operator
      "scope": [
				"keyword.operator.type",
				"keyword.control.as",
        "keyword.operator.relational",
				"meta.brace.round",
				"meta.brace.angle",
				"meta.brace.square",
        "meta.return.type",
				"storage.type.function.arrow",
				"punctuation",
        "fenced_code.block.language"
      ],
      "settings": {
        "foreground": "#A5C0AA"
      }
    },
    {
      // ts types
      "scope":[
				"support.type",
				"entity.name.type",
				"meta.interface"
      ],
      "settings": {
        "foreground": "#07beb8"
      }
    },
		{
      // comment
			"scope": [
				"comment",
        "punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#70737B"
			}
		},
		{
			"scope": [
        "markup.heading",
        "punctuation.definition.heading"
      ],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d4976cf2"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
			}
		},
		{
			"scope": "markup.italic markup.bold | markup.bold markup.italic",
			"settings": {
				"fontStyle": "bold italic"
			}
		},
		{
			"scope": "markup.list.numbered.bullet",
			"settings": {
				"foreground": "#A3CE9E"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#99C794"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#EBCB8B"
			}
		},
		{
			"scope": "message.error",
			"settings": {
				"foreground": "#EC5F66"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796E6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#CD9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#F44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#B267E6"
			}
		}
	]
}
