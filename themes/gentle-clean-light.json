  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#b05a78",
    "property": "#998418",
    "interface": "#2e808f",
    "type": "#2e808f",
    "class": "#5a6aa6"
  },
	"tokenColors": [
		{
      // key
			"scope": [
				"entity.name.label",
				"entity.other.attribute-name",
				"meta.directive",
				"meta.tag.block.any",
        "meta.object-literal.key",
				"support.type.property-name",
				"variable.object.property",
        "variable.other.object.property",
        "variable.other.property",
        "variable.other.enummember",
				"punctuation.attribute-shorthand"
			],
			"settings": {
				"foreground": "#028dac"
			}
		},
		{
      // string
			"scope": [
				"string",
				"source",
				"text.html",
				"text.html.derivative",
				"meta.property-value",
				"keyword.other.unit",
				"support.type",
				"support.constant.property-value",
				"constant.numeric.css",
				"constant.other.color.rgb-value.hex.css"
			],
			"settings": {
				"foreground": "#017e5e"
			}
		},
		{
      // keyword
			"scope": [
        "punctuation.definition.entity.css",
				"entity.name.tag",
				"entity.other.attribute-name.class",
        "entity.other.attribute-name.pseudo-class.css",
				"support.class.component",
				"storage",
				"keyword",
				"keyword.control",
				"keyword.operator",
				"keyword.operator.word",
				"keyword.operator.new.js",
        "keyword.operator.relational",
				"variable.language",
        "variable.scss"
			],
			"settings": {
				"foreground": "#ab6200"
			}
		},
		{
      // variable
			"scope": [
				"constant",
				"variable.member",
				"variable.parameter",
				"variable.other.constant.object",
				"variable.other.constant.property",
				"variable.other.constant",
				"variable.other.object",
				"variable.other.readwrite",
				"meta.definition.variable",
				"meta.type.declaration.ts",
				"support.constant",
				"support.variable.property",
				"meta.diff",
				"meta.diff.header",
        "string.regexp"
			],
			"settings": {
				"foreground": "#c44285"
			}
		},
		{
      // funciton
			"scope": [
				"variable.function",
				"variable.annotation",
				"support.function",
				"support.macro",
				"support.class",
				"entity.name.function",
				"string.other.link",
				"markup.underline.link",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.accessor.optional",
        "meta.command",
        "string.unquoted.argument"
			],
			"settings": {
				"foreground": "#266ad7"
			}
		},
    {
      // operator
      "scope": [
				"keyword.operator.type",
				"keyword.control.as",
				"meta.brace.round",
				"meta.brace.angle",
				"meta.brace.square",
        "meta.return.type",
				"storage.type.function.arrow",
				"punctuation",
        "fenced_code.block.language"
      ],
      "settings": {
        "foreground": "#8f9aa1"
      }
    },
    {
      // ts types
      "scope":[
				"support.type",
				"entity.name.type",
				"meta.interface"
      ],
      "settings": {
        "foreground": "#a14fc4"
      }
    },
		{
      // comment
			"scope": [
				"comment",
        "punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#8d94a6"
			}
		},
		{
			"scope": [
        "markup.heading",
        "punctuation.definition.heading"
      ],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d4976cf2"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
			}
		},
		{
			"scope": "markup.italic markup.bold | markup.bold markup.italic",
			"settings": {
				"fontStyle": "bold italic"
			}
		},
		{
			"scope": "markup.list.numbered.bullet",
			"settings": {
				"foreground": "#A3CE9E"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#99C794"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#EBCB8B"
			}
		},
		{
			"scope": "message.error",
			"settings": {
				"foreground": "#EC5F66"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796E6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#CD9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#F44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#B267E6"
			}
		}
	]
}
